{
  "name": "@ferrer/typescript-minimal",
  "version": "0.0.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "license": "UNLICENSED",
  "description": "Minimal skeleton for a TypeScript library or app.",
  "type": "module",
  "main": "dist/lib/index.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.4.0",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint src",
    "build:tsc": "tsc",
    "build:typings": "tsc -d --declarationDir dist/typings --declarationMap --emitDeclarationOnly src/index.ts",
    "test": "jest"
  }
}
